                    "401": {
                        "description": "Unauthorized",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "error": {
                                            "type": "string",
                                            "example": "Unauthorized"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/v1/predict": {
            "post": {
                "tags": [
                    "Predict"
                ],
                "summary": "Prediksi nilai akademik",
                "description": "Memprediksi nilai akademik berdasarkan data yang diinput pengguna.",
                "operationId": "9a0c662fc36f0bd2b5d293702dd8ba7e",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "name",
                                    "age",
                                    "gender",
                                    "education",
                                    "attendance",
                                    "hours_studied",
                                    "previous_scores",
                                    "sleep_hours",
                                    "tutoring_sessions",
                                    "peer_influence",
                                    "motivation_level",
                                    "teacher_quality",
                                    "access_to_resources"
                                ],
                                "properties": {
                                    "name": {
                                        "type": "text",
                                        "example": "John Doe"
                                    },
                                    "age": {
                                        "type": "number",
                                        "example": 15
                                    },
                                    "gender": {
                                        "type": "text",
                                        "example": "male"
                                    },
                                    "education": {
                                        "type": "text",
                                        "example": "SMK"
                                    },
                                    "subject_id": {
                                        "type": "number",
                                        "example": 1
                                    },
                                    "attendance": {
                                        "type": "number",
                                        "example": 90
                                    },
                                    "hours_studied": {
                                        "type": "number",
                                        "example": 5
                                    },
                                    "previous_scores": {
                                        "type": "number",
                                        "example": 75
                                    },
                                    "sleep_hours": {
                                        "type": "number",
                                        "example": 8
                                    },
                                    "tutoring_sessions": {
                                        "type": "integer",
                                        "example": 2
                                    },
                                    "peer_influence": {
                                        "type": "string",
                                        "enum": [
                                            "positive",
                                            "neutral",
                                            "negative"
                                        ],
                                        "example": "positive"
                                    },
                                    "motivation_level": {
                                        "type": "string",
                                        "enum": [
                                            "low",
                                            "medium",
                                            "high"
                                        ],
                                        "example": "high"
                                    },
                                    "teacher_quality": {
                                        "type": "string",
                                        "enum": [
                                            "low",
                                            "medium",
                                            "high"
                                        ],
                                        "example": "medium"
                                    },
                                    "access_to_resources": {
                                        "type": "string",
                                        "enum": [
                                            "low",
                                            "medium",
                                            "high"
                                        ],
                                        "example": "high"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Predicted successfully",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Predicted successfully"
                                        },
                                        "student": {
                                            "properties": {
                                                "id": {
                                                    "type": "integer",
                                                    "example": 1
                                                },
                                                "name": {
                                                    "type": "string",
                                                    "example": "John Doe"
                                                },
                                                "age": {
                                                    "type": "integer",
                                                    "example": 20
                                                },
                                                "gender": {
                                                    "type": "string",
                                                    "example": "male"
                                                },
                                                "education": {
                                                    "type": "string",
                                                    "example": "SMA"
                                                },
                                                "user_id": {
                                                    "type": "integer",
                                                    "example": null,
                                                    "nullable": true
                                                },
                                                "created_at": {
                                                    "type": "string",
                                                    "format": "date-time"
                                                },
                                                "updated_at": {
                                                    "type": "string",
                                                    "format": "date-time"
                                                }
                                            },
                                            "type": "object"
                                        },
                                        "record": {
                                            "properties": {
                                                "subject_id": {
                                                    "type": "integer",
                                                    "example": 1
                                                },
                                                "input_date": {
                                                    "type": "string",
                                                    "format": "date-time",
                                                    "example": "2025-04-11T08:31:54.812441Z"
                                                },
                                                "attendance": {
                                                    "type": "integer",
                                                    "example": 0
                                                },
                                                "hours_studied": {
                                                    "type": "number",
                                                    "format": "float",
                                                    "example": 0
                                                },
                                                "previous_scores": {
                                                    "type": "number",
                                                    "format": "float",
                                                    "example": 0
                                                },
                                                "sleep_hours": {
                                                    "type": "number",
                                                    "format": "float",
                                                    "example": 0
                                                },
                                                "tutoring_sessions": {
                                                    "type": "integer",
                                                    "example": 0
                                                },
                                                "peer_influence": {
                                                    "type": "string",
                                                    "example": "negative"
                                                },
                                                "motivation_level": {
                                                    "type": "string",
                                                    "example": "low"
                                                },
                                                "teacher_quality": {
                                                    "type": "string",
                                                    "example": "low"
                                                },
                                                "access_to_resources": {
                                                    "type": "string",
                                                    "example": "low"
                                                }
                                            },
                                            "type": "object"
                                        },
                                        "prediction_result": {
                                            "properties": {
                                                "prediction_date": {
                                                    "type": "string",
                                                    "format": "date-time",
                                                    "example": "2025-04-11T08:31:54.812490Z"
                                                },
                                                "predicted_score": {
                                                    "type": "number",
                                                    "format": "float",
                                                    "example": 85.58453369140625
                                                },
                                                "recommendation": {
                                                    "type": "string",
                                                    "example": "Excellent performance"
                                                }
                                            },
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Validation error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Validation error"
                                        },
                                        "errors": {
                                            "properties": {
                                                "attendance": {
                                                    "type": "array",
                                                    "items": {
                                                        "type": "string",
                                                        "example": "Attendance must be between 0 and 100."
                                                    }
                                                },
                                                "hours_studied": {
                                                    "type": "array",
                                                    "items": {
                                                        "type": "string",
                                                        "example": "Hours studied must be a number."
                                                    }
                                                }
                                            },
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
